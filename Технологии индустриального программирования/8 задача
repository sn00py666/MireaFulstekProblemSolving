#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    int n;
    int a;
    int b;
    int c;
    int d;
    
    cin >> n;
    
    a = n / 1000;
    b = n / 100 % 10;
    c = n / 10 % 10;
    d = n % 10;
    
    cout << int(a == d) * int(b == c);
                            
    return 0;
}
